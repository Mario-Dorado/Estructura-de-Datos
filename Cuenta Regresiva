def cuenta_regresiva(num):
    num-=1
    if num>0:
        print(num)
        cuenta_regresiva(num)
    else:
        print ("BOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOM!")
    print("Fin de la funci√≥n ", num)
    
cuenta_regresiva(num)
